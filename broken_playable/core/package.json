{
    "name": "my-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "purge": "npx rimraf ../dist",
        "start": "npm run purge && webpack serve --open --config webpack.dev.js",
        "format": "prettier --write \"src/*/*.ts\"",
        "build": "npm run purge && webpack --config webpack.deploy_preview.js",
        "build-al": "webpack --config webpack.deploy_al.js",
        "build-unity": "webpack --config webpack.deploy_unity.js",
        "build-is": "webpack --config webpack.deploy_is.js",
        "build-fb": "webpack --config webpack.deploy_fb.js",
        "build-tt": "webpack --config webpack.deploy_tt.js",
        "build-google": "npx rimraf ../dist/google && webpack --config webpack.deploy_google.js",
        "build-mindworks": "npx rimraf ../dist/mindworks && webpack --config webpack.deploy_mindworks.js",
        "build-moloco-file": "webpack --config webpack.deploy_moloco.js",
        "clear-moloco-xhr": "node ./scripts/clearXHR.js",
        "build-moloco": "npm run build-moloco-file && npm run clear-moloco-xhr",
        "build-snap": "webpack --config webpack.deploy_snap.js",
        "build-vungle": "webpack --config webpack.deploy_vungle.js",
        "build-prod": "npx update-browserslist-db@latest && npm run purge  && npm run build-al  && npm run build-unity  && npm run build-is  && npm run build-fb  && npm run build-google  && npm run build-mindworks && npm run build-moloco  && npm run build-vungle  && npm run clean-up",
        "clean-up": "npx rimraf ../dist/fb && npx rimraf ../dist/tt && npx rimraf ../dist/mindworks && npx rimraf ../dist/google &&npx rimraf ../dist/vungle"
    },
    "devDependencies": {
        "clean-webpack-plugin": "^4.0.0",
        "css-loader": "^6.7.3",
        "esbuild": "^0.12.14",
        "esbuild-loader": "^2.13.1",
        "esbuild-plugin-alias": "^0.1.2",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-webpack": "^0.13.2",
        "eslint-webpack-plugin": "^2.5.4",
        "file-loader": "^6.2.0",
        "html-inline-script-webpack-plugin": "^3.0.0",
        "html-webpack-plugin": "^5.3.2",
        "rimraf": "^3.0.2",
        "terser-webpack-plugin": "^5.1.4",
        "url-loader": "^4.1.1",
        "webpack": "^5.42.1",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.15.1",
        "webpack-merge": "^5.8.0",
        "zip-webpack-plugin": "^4.0.1"
    }
}
